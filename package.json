{
  "name": "archjs",
  "version": "0.1.0",
  "description": "Full-stack Node.js Framework",
  "keywords": [
    "nodejs",
    "uwebsockets",
    "svelte",
    "every-layout",
    "rollup",
    "full-stack",
    "framework"
  ],
  "author": "Artem Tolstykh aka Mr. Arch",
  "license": "MIT",
  "main": "dist/server.js",
  "scripts": {
    "develop": "sh etc/scripts/develop.sh",
    "start": "sh etc/scripts/start.sh",
    "clinic": "sh etc/scripts/clinic.sh",
    "perform": "sh etc/scripts/perform.sh profile/arch",
    "test": "standard && jest --watchAll",
    "storybook": "start-storybook -p 6006 -s static",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "abstract-logging": "*",
    "page": "*",
    "pino": "*",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v18.9.0"
  },
  "devDependencies": {
    "@babel/core": "*",
    "@babel/plugin-syntax-dynamic-import": "*",
    "@babel/plugin-transform-runtime": "*",
    "@babel/preset-env": "*",
    "@babel/runtime": "*",
    "@rollup/plugin-babel": "*",
    "@rollup/plugin-commonjs": "*",
    "@rollup/plugin-node-resolve": "*",
    "@rollup/plugin-replace": "*",
    "@storybook/addon-actions": "^6.0.27",
    "@storybook/addon-essentials": "^6.0.27",
    "@storybook/addon-links": "^6.0.27",
    "@storybook/addon-storyshots": "^6.0.27",
    "@storybook/svelte": "^6.0.27",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/svelte": "^3.0.0",
    "babel-loader": "*",
    "jest": "^26.6.1",
    "jest-transform-svelte": "^2.1.1",
    "nodemon": "*",
    "prettier": "*",
    "react-is": "^17.0.1",
    "rollup": "*",
    "rollup-plugin-livereload": "*",
    "rollup-plugin-svelte": "*",
    "rollup-plugin-terser": "*",
    "standard": "*",
    "svelte": "^3.29.4",
    "svelte-loader": "^2.13.6"
  },
  "standard": {
    "ignore": [
      "dist",
      "static"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.svelte$": "jest-transform-svelte"
    },
    "moduleFileExtensions": ["js", "svelte", "json"],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss|stylesheet)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFilesAfterEnv": ["@testing-library/jest-dom/extend-expect"],
    "testPathIgnorePatterns": ["/node_modules/", "/build/", "/storybook-static/"]
  },
  "engines": {
    "node": ">=14"
  },
  "private": true
}
